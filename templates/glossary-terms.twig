{% block terms %}
    <ul class="glossary-terms-list">
        {% for term in terms %}
            {% block term %}
                {{ include('glossary-terms-term.twig', {
                    term: term,
                    translations: translations,
                    languages: languages,
                    edit_enabled: edit_enabled
                    }) }}
            {% endblock %}
        {% endfor %}
    </ul>
{% endblock %}